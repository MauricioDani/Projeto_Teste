<thf-page-list t-title="<%= classify(name) %>" [t-actions]="actions">
<% if(dataView === 'table') { %>
  <thf-table
    t-container
    t-sort="true"
    t-striped="true"
    [t-columns]="columns"
    [t-items]="items"
    [t-height]="400">
  </thf-table>
<% } else { %>
  <thf-list-view
    t-property-title="name"
    [t-items]="items">

    <ng-template t-list-view-content-template let-item>
      <div class="thf-row">
        <thf-info class="thf-md-6" t-label="Age" [t-value]="item.age">
        </thf-info>

        <thf-info class="thf-md-6" t-label="Email" [t-value]="item.email">
        </thf-info>
      </div>
    </ng-template>

  </thf-list-view>
<% }%>
</thf-page-list>
